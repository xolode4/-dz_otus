[mysqld]
user            = mysql
bind-address            = 0.0.0.0
key_buffer_size         = 16M
myisam-recover-options  = BACKUP
log_error = /var/lib/mysql/error.log
server-id               = 1
gtid-mode=ON
enforce-gtid-consistency
log-replica-updates
binlog_expire_logs_seconds      = 2592000
max_binlog_size   = 100M
